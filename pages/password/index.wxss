/* pages/password/index.wxss */
page{
  height: 100%;
  background: #efefef;
}
.team{
  width: 100%;
  background: #fff;
  border-top: 1rpx solid #efefef;
  margin-top: 30rpx;
}
.team input{
  padding: 30rpx;
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 1rpx solid #efefef;
  box-sizing: border-box;
}
.btn{
  position: fixed;
  bottom: 0;
  width: 100%;
  line-height: 120rpx;
  text-align: center;
  font-size: 44rpx;
  background: #65b393;
  color: #fff;
  border: none;
  border-radius: none;
}