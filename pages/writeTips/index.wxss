/* pages/writeTips/index.wxss */
.photo image{
  width: 100%;
  height: 700rpx;
}
.content{
  padding: 20rpx;
  box-sizing: border-box;
}
.content text{
  color: #999;
  font-size: 28rpx;
}
.address{
  float: right;
}
.content:after{clear:both;content:'.';display:block;height:0;width:0;visibility:hidden;}
.btn{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background: #65b393;
  color: #fff;
  font-size: 36rpx;
  text-align: center;
}