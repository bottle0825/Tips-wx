.title{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  height: 200rpx;
  margin-top: 20rpx;
  color: #fff;
  font-size: 36rpx;
  text-align: center;
}
.Input{
  padding: 0 103rpx;
}
.Input>view{
  display: flex;
  align-items: center;
  justify-content: center;
  width: 544rpx;
  height: 92rpx;
  background-color: rgba(255, 255, 255, 0.25);
  border-radius: 12rpx;
  padding: 0 20rpx 0 38rpx;
  margin-bottom: 27rpx;
  box-sizing: border-box;
}
.Check{
  width: 320rpx;
  line-height: 56rpx;
  border-radius: 28rpx;
  font-size: 28rpx;
  color: #fff;
  background: #3d9578;
  box-sizing: border-box;
}
.Wait{
  width: 320rpx;
  line-height: 56rpx;
  border-radius: 28rpx;
  font-size: 28rpx;
  color: #2a2623;
  background: #f6f6f9;
  box-sizing: border-box;
}
input{
  width: 100%;
  color: #fff;
}
.pl{
  color: #fff;
  font-size: 28rpx;
}
.btn{
  margin-top: 67rpx;
  border-radius: 46rpx;
  color: #65b393;
  font-size: 36rpx;
}
.footer{
  position: fixed;
  bottom: 0;
  line-height: 105rpx;
  width: 100%;
  color: #fff;
  text-align: center;
  font-size: 30rpx;
  border-top: 1rpx solid #fff;
  background: #65b393;
}